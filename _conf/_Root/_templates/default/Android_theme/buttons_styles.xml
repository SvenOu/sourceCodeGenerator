<resources xmlns:android="http://schemas.android.com/apk/res/android"
           xmlns:tools="http://schemas.android.com/tools"
           tools:ignore="MissingPrefix">

    <style name="BUTTON">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/normal_btn_height</item>
    </style>
    $muti-repeat1(buttonGroups){{

    <style name="$(name-upCaseALL)" parent="$(parent-upCaseALL)">
        <item name="android:background">@drawable/$(background-upCaseALL)</item>
    </style>

    $muti-repeat1-1(colors){{

    <style name="$(name-upCaseALL)" parent="$(parent-upCaseALL)">
        <item name="android:textColor">@color/$(color)</item>
    </style>

    $muti-repeat1-1-1(fontFamilys){{
    <style name="$(name-upCaseALL)" parent="$(parent-upCaseALL)">
        <item name="fontPath">@string/$(fontFamily)</item>
    </style>

    $muti-repeat1-1-1-1(fontSizes){{
    <style name="$(name-upCaseALL)" parent="$(parent-upCaseALL)">
        <item name="android:textSize">@dimen/$(fontSize)</item>
    </style>
    $end1-1-1-1}}
    $end1-1-1}}
    $end1-1}}
    $end1}}
</resources>
